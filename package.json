{
  "name": "vite-starter",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.15.5",
  "scripts": {
    "dev": "vite --port 3333 --open",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "play": "pnpm -C playground run dev",
    "docs:dev": "pnpm -C docs run dev",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "prepare": "ignore-dependency-scripts \"tsx scripts/prepare.ts\"",
    "postinstall": "ignore-dependency-scripts \"stale-dep -u && pnpm prepare\"",
    "release": "stale-dep && bumpp && tsx scripts/release.ts",
    "up": "taze major -I",
    "update:team:avatars": "tsx scripts/avatars.ts"
  },
  "dependencies": {
    "@iconify-json/logos": "^1.1.42",
    "@iconify/json": "^2.2.195",
    "@vueuse/core": "^10.9.0",
    "floating-vue": "^5.2.2",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "screenfull": "^6.0.2",
    "vue": "^3.4.21",
    "vue-i18n": "^9.10.2",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.9.0",
    "@intlify/vite-plugin-vue-i18n": "^7.0.0",
    "@types/chroma-js": "^2.4.4",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^18.19.26",
    "@types/nprogress": "^0.2.3",
    "@unocss/eslint-plugin": "^0.58.6",
    "@unocss/reset": "^0.58.6",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/test-utils": "^2.4.5",
    "bumpp": "^9.4.0",
    "chroma-js": "^2.4.2",
    "cross-env": "^7.0.3",
    "cypress": "^13.7.1",
    "eslint": "^8.57.0",
    "fs-extra": "^11.2.0",
    "ignore-dependency-scripts": "^1.0.1",
    "jsdom": "^24.0.0",
    "ofetch": "^1.3.4",
    "simple-git": "^3.23.0",
    "simple-git-hooks": "^2.11.0",
    "stale-dep": "^0.7.0",
    "taze": "^0.13.3",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3",
    "unocss": "^0.58.6",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.2.6",
    "vite-plugin-pages": "^0.32.0",
    "vite-plugin-vue-devtools": "^7.0.20",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vitest": "^1.4.0",
    "vue-tsc": "^2.0.7"
  }
}
